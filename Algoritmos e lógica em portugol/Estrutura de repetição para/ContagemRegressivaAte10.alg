algoritmo "ContadorRegresivoAte10"
var
   N1, N2, Contador: Inteiro
inicio
      Escreva ("Digite um número: ")
              leia (N1)
      escreva ("Digite outro número: ")
              leia (N2)
      Para Contador <- N1 ate 0 passo -N2 faca
           escreval ("Contagem regressiva de ", N1, " até 0, pulando ", N2, " números: ", Contador)
      fimpara
fimalgoritmo

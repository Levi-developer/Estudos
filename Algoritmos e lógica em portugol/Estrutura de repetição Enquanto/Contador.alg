algoritmo "Contador"
var
   contador, n: inteiro
inicio
      contador <- 0
      ESCREVA ("Numero de contagens: ")
              leia (N)
      enquanto (contador <= n) faca
               escreval (contador)
               contador <- contador + 1
      fimenquanto

      Escreva ("TERMINEI DE CONTAR")
fimalgoritmo

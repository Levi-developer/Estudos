algoritmo "FazerSOMAprocedimento"
var
   X, Y: INTEIRO
procedimento Soma(A, B: inteiro)
inicio
      escreval ("Recebi o valor: ", A)
      escreval ("Recebi o valor: ", B)
      escreva ("A soma entre os  dois números é: ", A+B)

fimprocedimento
inicio
      X <- 10
      Y <- 20
      
      Soma(X, Y)
fimalgoritmo
